{
    "version": 2,
    "builds": [
      {
        "src": "project/wsgi.py",
        "use": "@vercel/python",
        "config" : {"maxLambdaSize" : "1024mb" , "runtime" : "python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "project/wsgi.py"
      }
    ]
  }
  